CLANG_CXX_LANGUAGE_STANDARD = c++14
CLANG_CXX_LIBRARY = compiler-default
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/hermes
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../../node_modules/react-native/hermes/destroot/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 HERMES_ENABLE_DEBUGGER=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/hermes" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/hermes"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../../node_modules/react-native/hermes
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
